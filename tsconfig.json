{
  "compilerOptions": {
    "types": ["express", "node", "lodash", "multer"],
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true
  },
  "include": ["src/**/*.ts", "public/**/*.html"],
  "exclude": ["node_modules"],
  "moduleResolution": "node"
}
